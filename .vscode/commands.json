{
  "commands": [
    {
      "text": "$(gear)",
      "command": "workbench.action.openGlobalSettings",
      "tooltip": "Settings"
    },
    {
      "command": "commands.refresh",
      "text": "$(sync)",
      "tooltip": "Refresh commands",
      "color": "#FFCC00"
    },
    {
      "text": "$(tools)Build",
      "command": "workbench.action.tasks.runTask",
      "arguments": ["Build"],
      "tooltip": "Build & Run",
      "color": "#00f30c"
    },
    {
      "text": "$(play)Run ($(bug))",
      "command": "workbench.action.tasks.runTask",
      "arguments": ["Run Debug"],
      "tooltip": "Build & Run",
      "color": "#00f30c"
    },
    {
      "text": "$(play)Run ($(zap))",
      "command": "workbench.action.tasks.runTask",
      "arguments": ["Run Release"],
      "tooltip": "Build & Run",
      "color": "#ff009d"
    },
    {
      "text": "$(beaker) UnitTests",
      "command": "workbench.action.tasks.runTask",
      "arguments": ["Run Tests"],
      "tooltip": "Run Unit tests",
      "color": "#ff009d"
    },
    {
      "text": "$(github) Push",
      "command": "workbench.action.tasks.runTask",
      "arguments": ["Push"],
      "tooltip": "Push to github",
      "color": "#2a8b7b"
    }
  ]
}